let num = [5,8,2,9,3]
console.log(`Nosso vetor é o ${num}`) 
// Ou console.log(num) Pra aparecer o colchete também

num.sort() //Ordena os elementos em ordem crescente

num.push(1) //Note que aqui ele está adicionando 1 após o num.sort, então o 1 entra no final do vetor...

console.log(`Nosso vetor tem ${num.length} elementos`) //Demonstra a quantidade de elementos dentro de um vetor

console.log(`O primeiro elemento do vetor é ${num[0]}`)

console.log(num)

let pos = num.indexOf(8)
console.log(`O valor 8 está na posição ${pos}`)

let vazia = num.indexOf(12)
if (vazia == -1){
    console.log(`O valor 12 não existe no vetor`)
} 
